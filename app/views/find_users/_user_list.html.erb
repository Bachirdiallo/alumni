<div id="result">
  <table class="table table-striped table-hover table-bordered">
    <thead>
      <tr>
        <th >Name</th>
        <th ></th>
        <th ></th>
        <th >Position</th>
        <th >Company</th>
        <th >Batch</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |search_result| %>
        <tr>
          <% if search_result.is_graduate %>
            <% if search_result.id != current_user.id %>
                <% if search_result.location_privacy_option != "Only Me" %>
                  <% if search_result.location_privacy_option == "My Batch" %>
                    <% if current_user.graduations.map{|x| x.batch} & search_result.graduations.map{|s| s.batch}%>
                        <td><%= search_result.name%></td>
                        <td><%= search_result.city%></td>
                        <td><%= search_result.country%></td>
                        <% if search_result.job_privacy_otpion != "Only Me" %>
                            <%if search_result.job_privacy_otpion == "Everyone"%>
                                <% search_result.jobs.each do |job|%>
                                  <% if job.current == "1" %>
                                    <td><%= job.title %></td>
                                    <td><%= job.company %></td>
                                  <% end %>
                                <% end %>
                            <%elsif  search_result.job_privacy_otpion == "My Batch"%>
                                <% search_result.jobs.each do |job|%>
                                  <% if current_user.graduations.map { |b| b.batch}&job.graduations.map { |s| s.batch }%>
                                    <td><%= job.title %></td>
                                    <td><%= job.company %></td>
                                  <% end %>
                                <% end %>
                            <%elsif  search_result.job_privacy_otpion == "My Programme"%>
                                  <% search_result.jobs.each do |job|%>
                                    <% if current_user.graduations.map { |p| p.programme  }& search_result.graduations.map { |p| p.programme  } %>
                                      <td><%= job.title %></td>
                                      <td><%= job.company %></td>
                                    <% end %>
                                  <% end %>
                            <%end%>
                        <% end %>
                <% elsif search_result.location_privacy_option == "My Programme"%>
                    <% if current_user.graduations.map { |n| n.programme  } & search_result.graduations.map { |n| n.programme }%>
                        <td><%= search_result.name%></td>
                        <td><%= search_result.city%></td>
                        <td><%= search_result.country%></td>

                        <% if search_result.job_privacy_otpion != "Only Me"%>
                          <%if search_result.job_privacy_otpion == "Everyone"%>
                              <% search_result.jobs.each do |job|%>
                                <% if job.current == "1" %>
                                  <td><%= job.title %></td>
                                  <td><%= job.company %></td>
                                <% end %>
                              <% end %>
                              <%elsif  search_result.job_privacy_otpion == "My Batch"%>
                                    <% if current_user.graduations.map { |b| b.batch  } & search_result.graduations.map { |b| b.batch  }%>
                                        <% search_result.jobs.each do |job|%>
                                            <td><%= job.title %></td>
                                            <td><%= job.company %></td>
                                        <% end %>
                                    <% end %>
                              <%elsif  search_result.job_privacy_otpion == "My Programme"%>
                                  <% search_result.jobs.each do |job|%>
                                    <% if current_user.graduations.map { |p|  p.programme} & search_result.graduations.map { |s|  s.programme} %>
                                      <td><%= job.title %></td>
                                      <td><%= job.company %></td>
                                    <% end %>
                                  <% end %>
                                <%end%>
                              <% end %>
                           <% end %>
                <% elsif search_result.location_privacy_option == "Everyone" %>
                    <td><%= search_result.name%></td>
                    <td><%= search_result.city%></td>
                    <td><%= search_result.country%></td>
                    <%if search_result.job_privacy_otpion == "Everyone"%>
                        <% search_result.jobs.each do |job|%>
                          <% if job.current == "1" %>
                            <td><%= job.title %></td>
                            <td><%= job.company %></td>
                          <% end %>
                        <% end %>
                    <%elsif  search_result.job_privacy_otpion == "My Batch"%>
                        <% search_result.jobs.each do |job|%>
                          <% if current_user.graduations.map { |b| b.batch  }& search_result.graduations.map { |e|  e.batch}%>
                            <td><%= job.title %></td>
                            <td><%= job.company %></td>
                          <% end %>
                        <% end %>
                    <%elsif  search_result.job_privacy_otpion == "My Programme"%>
                        <% search_result.jobs.each do |job|%>
                          <% if current_user.graduations.map { |p| p.programme  }& search_result.graduations.map { |p| p.programme  } %>
                            <td><%= job.title %></td>
                            <td><%= job.company %></td>
                          <% end %>
                        <% end %>
                      <%end%>
                <% end %>
               <% end %>
             <% end %>
          <% end %>
          </tr>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
