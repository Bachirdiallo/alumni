<!DOCTYPE html>
<html>
  <head>
    <title>Alumni</title>
    <%= stylesheet_link_tag    'application'%>
    <%= csrf_meta_tags %>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Cedarville+Cursive" />
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Aladin" />
    <script src="//maps.google.com/maps/api/js?v=3.23&sensor=false&client=&key=&libraries=geometry&language=&hl=&region="></script>
     <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
     <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->

  </head>
    <body>
      <div class="wrapper">
        <%= render 'layouts/header' %>
        <% if user_signed_in?%>
          <% if !current_user.is_graduate && !current_user.is_employer %>
            <div class="side-bar col-md-2">
              <%= render 'layouts/sidebar' %>
            </div>
          <%end%>
        <%end%>
        <% if user_signed_in?%>
          <% if !current_user.is_graduate && !current_user.is_employer   %>
            <div class=" cold-md-10">
              <div class="container pull-right" style="padding-right: 0px">
                <%= yield %>
              </div>
            </div>
          <%else%>
            <%= yield %>
          <%end%>
        <%else%>
          <%= yield %>
        <%end%>
        <% if !user_signed_in? %>
          <%= render partial: 'layouts/footer' %>
        <%end%>
      </div>
        <%= javascript_include_tag 'application'%>
    </body>

    <script>

      jQuery(document).ready(function() {
          App.init();
          OwlCarousel.initOwlCarousel();
          ParallaxSlider.initParallaxSlider();
          App.initParallaxBg();
          App.initScrollBar();
          Datepicker.initDatepicker();
          CirclesMaster.initCirclesMaster1();
          App.initSidebarMenuDropdown();
          window.map();
      });

    </script>
</html>
