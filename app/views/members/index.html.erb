
      <div class=" container-top">
        <% if !notice.nil? %>
          <div class="alert alert-info fade in">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
            <strong><%= notice %></strong>
          </div>
        <%end%>
        <div class="pull-left">
          <h2>Admin</h2>
        </div>
        <div class="col-md-2 pull-right" style="padding-top: 5px">
          <%= link_to new_member_path, title: 'Add User', :class => 'btn-u btn-u-light-green rounded pull-right' do%>
            <span class="fa fa-plus"></span>
          <%end%>
        </div>
      </div>


        <div class="main-content">
          <% if @users.any? %>
              <% @users.each_with_index do |user, i| %>
                <% if !user.is_graduate && !user.is_employer%>
                  <div class="lv-item item">
                    <div class="col-md-2" style="padding-left: 0px">
                      <%= user.student_id%>
                    </div>
                    <div class="col-md-3" style="padding-left: 0px">
                      <%= user.name%>
                    </div>
                    <div class="col-md-3" style="padding-left: 0px">
                      <%= user.email%>
                    </div>
                    <div class="col-md-2 ">
                      <%= link_to edit_member_path(user),  style: 'color: grey', class: 'pull-right' do %><span class="glyphicon glyphicon-pencil"></span> <% end %>
                    </div>
                    <div class="col-md-2 pull-right">
                      <%= link_to member_path(id: user.id), class: 'pull-right', style: 'color: grey', method: :delete, data: { confirm: 'Are you sure?' } do %><span class="fa fa-trash-o"></span> <% end %>
                    </div>
                    <br>
                  </div>
                    <% if i > 1 %>
                      <hr>
                    <%end%>
                <% end %>
              <% end %>
            <% end %>
        </div>
      </div>
    </div>
