<p><strong><%= notice %></strong></p>
<div class="container col-sm-8">
  <div>
    <h1>Admin list</h1>
    <table class="table table-striped table-hover table-bordered">
      <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr >
            <% if !user.is_graduate %>
                <% if !user.nil? %>
                  <td><%= user.student_id %></td>
                  <td><%= user.name %> </td>
                  <td><%= user.email %> </td>
                  <td><%= link_to 'Edit', edit_member_path(user) %></td>
                <% end %>
            <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <div>
    <%= link_to 'New User', new_member_path, :class => 'btn btn-primary btn-flat' %>
  </div>
</div>
