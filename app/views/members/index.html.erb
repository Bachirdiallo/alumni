<p><strong><%= notice %></strong></p>
<div class="container">
  <div class="col-sm-8">
    <h1>Admin list</h1>
    <table class="table table-striped table-hover ">
      <tbody>
        <% @users.each do |user| %>
          <tr >
            <% if !user.is_graduate %>
                <% if !user.nil? %>
                  <td><%= user.student_id %></td>
                  <td><%= user.name %> </td>
                  <td><%= user.email %> </td>
                  <td><%= link_to 'Edit', edit_member_path(user) %></td>
                <% end %>
            <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
    <div>
      <%= link_to 'New User', new_member_path, :class => 'btn btn-primary btn-md' %>
    </div>
  </div>

</div>

<style media="screen">
  h1{color: #009688;}
</style>
