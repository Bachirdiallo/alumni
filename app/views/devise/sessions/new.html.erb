<div class="col-sm-4"></div>
<div class="col-sm-4">
  <div class="panel panel-primary">
    <div class="panel-heading">
      Log in
    </div>
    <div class="panel-body">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form-control-wrapper" >
          <%= f.text_field :student_id, autofocus: true, autocomplete: "off", :class => 'form-control empty' %>
          <div class="floating-label">id </div>
            <span class="material-input"></span>
        </div>

        <div class="form-control-wrapper" >
          <%= f.password_field :password, autofocus: false, autocomplete: "off", :class => 'form-control empty' %>
          <div class="floating-label">password </div>
            <span class="material-input"></span>
        </div>

        <% if devise_mapping.rememberable? -%>
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me %>
        <% end -%>
        <div>
          <%= f.submit "Log in", :class => 'btn btn-primary btn-md' %>
        </div>
      <% end %>
    </div>
  </div>
</div>
